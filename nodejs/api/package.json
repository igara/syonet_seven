{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development sls offline --httpPort 3001",
    "testdb:create": "bin/mysqldef -h localhost -P 3306 -u root -p $MYSQL_ROOT_PASSWORD testdb < src/models/typeorm/migrations/schema.sql",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate:development": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register --project tsconfig.json ./node_modules/typeorm/cli.js -c default migration:generate -n migration",
    "typeorm:migration:run:development": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register --project tsconfig.json ./node_modules/typeorm/cli.js -c default migration:run",
    "mysqldef:migration:export:development": "src/bin/mysqldef -h mysql -P 3306 -u $MYSQL_USER -p $MYSQL_PASSWORD $MYSQL_DATABASE --export > src/models/typeorm/migrations/schema.sql",
    "mysqldef:migration:import:development": "src/bin/mysqldef -h mysql -P 3306 -u $MYSQL_USER -p $MYSQL_PASSWORD $MYSQL_DATABASE < src/models/typeorm/migrations/schema.sql",
    "typeorm:migration:generate:production": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --project tsconfig.json ./node_modules/typeorm/cli.js -c default migration:generate -n migration",
    "typeorm:migration:run:production": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register --project tsconfig.json ./node_modules/typeorm/cli.js -c default migration:run",
    "create:domain": "sls create_domain --stage production",
    "deploy": "cross-env NODE_ENV=production sls deploy --stage production",
    "close": "sls remove --stage production"
  },
  "author": "",
  "license": "ISC",
