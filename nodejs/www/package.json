{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "ts-node -r tsconfig-paths/register --project server.tsconfig.json server.ts",
    "dev": "NODE_ENV=development npm run sw:build && npm run start",
    "debug": "TS_NODE_PROJECT=server.tsconfig.json node --inspect-brk=0.0.0.0:9228 -r tsconfig-paths/register -r ts-node/register server.ts",
    "production": "npm run clean:build && NODE_ENV=production npm run sw:build && npm run build && npm run start",
    "stylelint": "stylelint styles/**/*.css",
    "eslint": "eslint . --ext .ts --ext .tsx",
    "lint": "npm run stylelint && npm run eslint",
    "clean:all": "rm -rf node_modules .next dist",
    "clean:build": "rm -rf .next dist",
    "sw:build": "NODE_OPTIONS=--max_old_space_size=2048 webpack",
    "test": "npm run testdata && jest --detectOpenHandles --forceExit",
    "coverage": "npm run testdata && jest --coverage --detectOpenHandles --forceExit",
    "testdata": "ts-node -r tsconfig-paths/register --project server.tsconfig.json libs/testdata.ts",
    "codeclimate": "cat ./coverage/lcov.info | ./node_modules/codeclimate-test-reporter/bin/codeclimate.js",
    "typeorm:migration:generate": "ts-node -r tsconfig-paths/register --project server.tsconfig.json $(npm bin)/typeorm migration:generate -n migration",
    "typeorm:migration:run": "ts-node -r tsconfig-paths/register --project server.tsconfig.json $(npm bin)/typeorm migration:run",
    "mysqldef:migration:export": "bin/mysqldef -h mysql -P 3306 -u $MYSQL_USER -p $MYSQL_PASSWORD $MYSQL_DATABASE --export > models/typeorm/migrations/schema.sql",
    "mysqldef:migration:import": "bin/mysqldef -h mysql -P 3306 -u $MYSQL_USER -p $MYSQL_PASSWORD $MYSQL_DATABASE < models/typeorm/migrations/schema.sql"
  },
  "dependencies": {
