{
	"name": "www",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm server",
		"server": "babel-node ./bin/www.js",
		"server_dev": "nodemon --exec babel-node ./bin/www.js",
		"syonet_build": "parcel build frontend/syonet/index.html -d dist/prod/syonet --public-url /syonet",
		"admin_build": "parcel build frontend/admin/index.html -d dist/prod/admin --public-url /admin",
		"lint": "eslint ./*",
		"local_syonet_sw_start": "parcel frontend/syonet/index.html -d dist/local/syonet -p 1111",
		"local_syonet_start": "parcel --https frontend/syonet/index.html -d dist/local/syonet -p 1111",
		"local_admin_start": "parcel --https frontend/admin/index.html -d dist/local/admin -p 1112",
		"flow": "flow",
		"test": "jest --detectOpenHandles --forceExit",
		"clear": "jest --clearCache && rm -rf .cache dist",
		"coverage": "jest --coverage --detectOpenHandles --forceExit",
		"testdata": "TEST=test babel-node libs/testdata.js"
	},
	"dependencies": {
		"@tensorflow/tfjs": "^0.12.2",
		"apollo-cache-inmemory": "^1.2.4",
		"apollo-client": "^2.3.4",
		"apollo-link-http": "^1.5.4",
		"apollo-server-express": "^2.2.3",
		"body-parser": "~1.18.3",
		"compression": "^1.7.2",
		"connect-mongo": "^2.0.1",
		"cookie-parser": "~1.4.3",
		"date-fns": "^1.29.0",
		"debug": "~3.1.0",
		"express": "~4.16.3",
		"express-session": "^1.15.6",
		"graphql": "^0.13.2",
		"graphql-tag": "^2.9.2",
		"graphql-tools": "^3.0.2",
		"isomorphic-fetch": "^2.2.1",
		"js-cookie": "^2.2.0",
		"mithril": "^1.1.6",
		"mongoose": "^5.3.13",
		"morgan": "~1.9.0",
		"passport": "^0.4.0",
		"passport-facebook": "^2.1.1",
		"passport-github": "^1.1.0",
		"passport-google-oauth20": "^1.0.0",
		"passport-twitter": "^1.0.4",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"react-router-dom": "^4.3.1",
		"serve-favicon": "~2.5.0",
		"styled-components": "^3.4.5",
		"web-push": "^3.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.1.6",
		"@babel/node": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-transform-flow-strip-types": "^7.1.6",
		"@babel/plugin-transform-react-jsx": "^7.1.6",
		"@babel/plugin-transform-runtime": "^7.1.0",
		"@babel/preset-env": "^7.1.6",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.0.0",
		"@babel/runtime": "^7.1.5",
		"@types/mithril": "^1.1.12",
		"autoprefixer": "^8.6.5",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^23.6.0",
		"codeclimate-test-reporter": "^0.5.0",
		"eslint": "^5.9.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-flowtype": "^3.2.0",
		"eslint-plugin-jest": "^22.0.0",
		"eslint-plugin-prettier": "^3.0.0",
		"eslint-plugin-react": "^7.11.1",
		"flow-bin": "^0.86.0",
		"flow-typed": "^2.5.1",
		"jest": "^23.6.0",
		"jest-environment-jsdom": "^23.1.0",
		"jest-environment-jsdom-global": "^1.1.0",
		"nodemon": "^1.18.6",
		"parcel-bundler": "^1.10.3",
		"parcel-plugin-inlinesvg": "^0.0.14",
		"parcel-plugin-sw-precache": "^1.0.2",
		"postcss-import": "^11.1.0",
		"postcss-modules": "^1.1.0",
		"postcss-simple-vars": "^4.1.0",
		"prettier": "^1.15.2"
	},
	"sw-precache": {
		"maximumFileSizeToCacheInBytes": 10485760,
		"navigateFallbackWhitelist": [
			"/^/auth//",
			"/^/manage//"
		]
	}
}
