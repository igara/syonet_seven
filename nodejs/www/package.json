{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production next build && webpack -c webpack.lambda.config.js",
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register --project server.tsconfig.json app-dev-server.ts",
    "serverless": "cross-env NODE_ENV=production serverless",
    "debug": "TS_NODE_PROJECT=server.tsconfig.json node --inspect-brk=0.0.0.0:9228 -r tsconfig-paths/register -r ts-node/register app-dev-server.ts",
    "stylelint": "stylelint styles/**/*.css",
    "eslint": "eslint . --ext .ts --ext .tsx",
    "lint": "npm run stylelint && npm run eslint",
    "clean:all": "rm -rf node_modules .next public/ogp public/work*",
    "test": "jest --detectOpenHandles --forceExit",
    "coverage": "jest --coverage --detectOpenHandles --forceExit",
    "testdb:create": "bin/mysqldef -h localhost -P 3306 -u root -p $MYSQL_ROOT_PASSWORD testdb < models/typeorm/migrations/schema.sql",
    "typeorm:migration:generate": "ts-node -r tsconfig-paths/register --project server.tsconfig.json $(npm bin)/typeorm -c default migration:generate -n migration",
    "typeorm:migration:run": "ts-node -r tsconfig-paths/register --project server.tsconfig.json $(npm bin)/typeorm -c default migration:run",
    "mysqldef:migration:export": "bin/mysqldef -h mysql -P 3306 -u $MYSQL_USER -p $MYSQL_PASSWORD $MYSQL_DATABASE --export > models/typeorm/migrations/schema.sql",
    "mysqldef:migration:import": "bin/mysqldef -h mysql -P 3306 -u $MYSQL_USER -p $MYSQL_PASSWORD $MYSQL_DATABASE < models/typeorm/migrations/schema.sql"
  },
