{
	"name": "www",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm server",
		"server": "nodemon --config nodemon_production.json",
		"server_dev": "nodemon --config nodemon_dev.json",
		"syonet_build": "parcel build frontend/syonet/index.html -d dist/prod/syonet --public-url /syonet --no-source-maps",
		"admin_build": "parcel build frontend/admin/index.html -d dist/prod/admin --public-url /admin --no-source-maps",
		"lint": "eslint ./*",
		"local_syonet_sw_start": "parcel frontend/syonet/index.html -d dist/local/syonet -p 1111",
		"local_syonet_start": "parcel --https frontend/syonet/index.html -d dist/local/syonet -p 1111",
		"local_admin_start": "parcel --https frontend/admin/index.html -d dist/local/admin -p 1112",
		"test": "TEST=test jest --detectOpenHandles --forceExit",
		"clear": "jest --clearCache && rm -rf .cache dist",
		"coverage": "TEST=test jest --coverage --detectOpenHandles --forceExit",
		"testdata": "TEST=testdata node -r tsconfig-paths/register -r ts-node/register libs/testdata.ts",
		"csstyped": "tcm -p **/*.pcss"
	},
	"dependencies": {
		"apollo-cache-inmemory": "^1.2.4",
