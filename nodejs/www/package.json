    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register --project server.tsconfig.json server.ts",
    "deploy": "cross-env NODE_ENV=production serverless",
    "close": "cross-env NODE_ENV=production serverless remove",
    "debug": "TS_NODE_PROJECT=server.tsconfig.json node --inspect-brk=0.0.0.0:9228 -r tsconfig-paths/register -r ts-node/register server.ts",
    "stylelint": "stylelint styles/**/*.css",
    "eslint": "eslint . --ext .ts --ext .tsx",
    "lint": "npm run stylelint && npm run eslint",
    "clean:all": "rm -rf node_modules .next public/ogp public/work*",
    "test": "jest --detectOpenHandles --forceExit",
    "coverage": "jest --coverage --detectOpenHandles --forceExit"
  },
