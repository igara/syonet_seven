{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "ts-node -r tsconfig-paths/register --project server.tsconfig.json server.ts",
    "dev": "NODE_ENV=localhost npm run sw:build && npm run start",
    "debug": "TS_NODE_PROJECT=server.tsconfig.json node --inspect-brk=0.0.0.0:9228 -r tsconfig-paths/register -r ts-node/register server.ts",
    "production": "NODE_ENV=production npm run sw:build && npm run build && npm run start",
    "stylelint": "stylelint styles/**/*.css",
    "eslint": "eslint . --ext .ts --ext .tsx",
    "lint": "npm run stylelint && npm run eslint",
    "clean": "rm -rf node_modules .next dist",
    "sw:build": "webpack",
    "test": "npm run testdata && jest --detectOpenHandles --forceExit",
    "coverage": "npm run testdata && jest --coverage --detectOpenHandles --forceExit",
    "testdata": "ts-node -r tsconfig-paths/register --project server.tsconfig.json libs/testdata.ts",
    "codeclimate": "cat ./coverage/lcov.info | ./node_modules/codeclimate-test-reporter/bin/codeclimate.js"
  },
  "dependencies": {
    "body-parser": "1.19.0",
