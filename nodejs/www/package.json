    "passport-twitter": "1.0.4",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-redux": "7.1.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "typescript-fsa": "3.0.0",
    "typescript-fsa-reducers": "1.2.1",
    "typescript-fsa-redux-thunk": "2.1.7",
