{
	"name": "www",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "pm2 --no-daemon start pm2.json",
		"server": "node -r babel-register ./bin/www.js",
		"server_debug": "node --inspect=0.0.0.0:9229 -r babel-register ./bin/www.js",
		"server_prod": "pm2 --no-daemon start npm -- server",
		"stop": "pm2 stop www",
		"pm2": "pm2",
		"syonet_build": "parcel build frontend/syonet/index.html -d dist/prod/syonet --public-url /syonet",
		"admin_build": "parcel build frontend/admin/index.html -d dist/prod/admin --public-url /admin",
		"lint": "eslint ./*",
		"local_syonet_sw_start": "parcel frontend/syonet/index.html -d dist/local/syonet -p 1111",
		"local_syonet_start": "parcel --https frontend/syonet/index.html -d dist/local/syonet -p 1111",
		"local_admin_start": "parcel --https frontend/admin/index.html -d dist/local/admin -p 1112",
		"flow": "flow",
		"test": "jest --detectOpenHandles --forceExit",
		"clear": "jest --clearCache && rm -rf .cache dist",
		"coverage": "jest --coverage --detectOpenHandles --forceExit",
		"testdata": "TEST=test node -r babel-register libs/testdata.js"
	},
	"dependencies": {
		"@tensorflow/tfjs": "^0.12.2",
		"apollo-cache-inmemory": "^1.2.4",
		"apollo-client": "^2.3.4",
		"apollo-link-http": "^1.5.4",
		"apollo-server-express": "^1.3.6",
		"body-parser": "~1.18.3",
		"compression": "^1.7.2",
		"connect-mongo": "^2.0.1",
		"cookie-parser": "~1.4.3",
		"date-fns": "^1.29.0",
		"debug": "~3.1.0",
		"express": "~4.16.3",
		"express-session": "^1.15.6",
		"graphql": "^0.13.2",
		"graphql-tag": "^2.9.2",
		"graphql-tools": "^3.0.2",
		"isomorphic-fetch": "^2.2.1",
		"js-cookie": "^2.2.0",
		"mithril": "^1.1.6",
		"mongoose": "^5.1.5",
		"morgan": "~1.9.0",
		"passport": "^0.4.0",
		"passport-facebook": "^2.1.1",
		"passport-github": "^1.1.0",
		"passport-google-oauth20": "^1.0.0",
		"passport-twitter": "^1.0.4",
		"pm2": "^2.10.4",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.3.1",
		"redux": "^4.0.0",
		"redux-actions": "^2.6.1",
		"redux-thunk": "^2.3.0",
		"serve-favicon": "~2.5.0",
		"styled-components": "^3.4.5"
	},
	"devDependencies": {
		"@types/mithril": "^1.1.12",
		"autoprefixer": "^8.6.5",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-jest": "^23.0.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-flow-strip-types": "^6.22.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-flow": "^6.23.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"babel-register": "^6.26.0",
		"babel-runtime": "^6.26.0",
		"codeclimate-test-reporter": "^0.5.0",
		"create-react-app": "^1.5.2",
		"eslint": "^5.0.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-flowtype": "^3.2.0",
		"eslint-plugin-jest": "^21.17.0",
		"eslint-plugin-prettier": "^3.0.0",
		"eslint-plugin-react": "^7.9.1",
		"flow-bin": "^0.86.0",
		"flow-typed": "^2.5.1",
		"jest": "^23.1.0",
		"jest-environment-jsdom": "^23.1.0",
